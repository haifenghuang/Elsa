import "std/io";

struct String
{
	char[] str;

	func char CharAt(int i)
	{
		return str[i];
	}

	func int Length()
	{
		return str.Length();
	}

	func bool Equals(String other)
	{
		if(Length() != other.Length())
		{
			return false;
		}
	
		loop(var i = 0; i < Length(); i++)
		{
			if(str[i] != other.str[i])
			{
				return false;
			}
		}
	
		return true;
	}
};
