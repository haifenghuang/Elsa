import "std/io";
import "std/assert";

struct MyStruct
{
	int x;

	func Get() : int
	{
		return x;
	}
	
	func Inc()
	{
		x++;
	}
};

func IncX2(func inc)
{
	inc();
	inc();
}

func main()
{
	var x = 10;

	var ret = func : int => { return x; };
	var inc = func => { x = x + 1; };

	AssertEq(10, ret());
	inc();
	AssertEq(11, ret());
	inc();
	AssertEq(12, ret());

	IncX2(inc);

	AssertEq(14, ret());

	PrintLn(":)");
}