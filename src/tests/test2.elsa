use "std/io";
use "std/assert";
use "std/window";

fn main() {
	var w = CreateWindowHandle("Test", 800, 600);
	OpenWindowHandle(w);
	
	while(true) {
		var msg = PeekMessage();
		while(msg != -1) {
			PrintLn(msg);
			msg = PeekMessage();
		}

		FillRect(w, 100, 100, 100, 450);
		FillRect(w, 200, 250, 100, 100);

		FillRect(w, 5, 5, 100, 100);
		FillRect(w, 695, 495, 100, 100);
		UpdateWindowHandle(w);
	}
}