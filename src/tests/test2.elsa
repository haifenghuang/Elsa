use "std/io";
use "std/assert";
use "std/window";

fn main() {
	var w = CreateWindow("Test", 800, 600);
	w.Open();

	while(true) {
		var msg = PeekMessage();
		while(msg != -1) {
			PrintLn(msg);
			msg = PeekMessage();
		}

		w.FillRect(100, 100, 100, 450);
		w.FillRect(200, 250, 100, 100);
		w.FillRect(5, 5, 100, 100);
		w.FillRect(695, 495, 100, 100);
		w.Update();
	}
}