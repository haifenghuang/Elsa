import "std/io";
import "std/assert";

func FuncCreateInvokeTest()
{
	var f1 = func (int x) : int => { return x + 100; };
	var f2 = func (int x, int y) : int => { return x + y; };
	var f4 = func (char c) => { AssertEq('X', c); };

	AssertEq(103, f1(3));
	AssertEq(140, f2(70, 70));
	f4('X');
}

struct MyStruct
{
	int x;

	func GetX() : int
	{
		return x;
	}
};

func ReturnInt() : int
{
	return 10;
}

func main()
{
	var str = new MyStruct { x: 10 };

	var gx = str.GetX;
	var ri = ReturnInt;

	PrintLn(gx());
	AssertEq(10, gx());
	PrintLn(gx());
	PrintLn(gx());
	str.x = 20;
	PrintLn(gx());
	PrintLn(gx());
	AssertEq(20, gx());
}