import "std/io";
import "std/assert";

struct MyStruct
{
	int x;

	fn Get() : int
	{
		return x;
	}
	
	fn Inc()
	{
		x++;
	}
};

fn IncX2(fn inc)
{
	inc();
	inc();
}

fn main()
{
	var x = 10;

	var ret = fn : int => { return x; };
	var inc = fn => { x = x + 1; };

	AssertEq(10, ret());
	inc();
	AssertEq(11, ret());
	inc();
	AssertEq(12, ret());

	IncX2(inc);

	AssertEq(14, ret());

	PrintLn(":)");
}